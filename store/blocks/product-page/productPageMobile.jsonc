  // "product-specification-text#conteudoEspecial-mobile": {
  //     "props": {
  //         "message": "{specificationValue}",
  //         "blockClass": "conteudoEspecial"
  //     }
  // },
  // "product-specification-group#conteudoEspecial-mobile" : {
  //   "children": ["product-specification#conteudoEspecial-mobile"],
  //   "props": {
  //     "filter": {
  //       "specificationGroups": "Especificação especial",
  //       "type": "show"
  //     }
  //   }
  // },
  "flex-layout.row#conteudo-especial-mobile": {
    "props": {
      "blockClass": "iframe-container"
    }
  },
  "flex-layout.row#accordion-mobile": {
    "props": {
      "blockClass": "accordion-pdp-desktop"
    },
    "children": ["disclosure-layout#accordion-product-mobile"]
  },
  "disclosure-layout#accordion-product-mobile": {
    "children": [
      "disclosure-layout#conteudo-especial-mobile",
      "disclosure-layout#description-mobile",
      "disclosure-layout#ficha-tecnica-mobile"
    ]
  },

  "disclosure-layout#description-mobile": {
    "children": ["disclosure-trigger#description-mobile", "disclosure-content#description-mobile"],
    "props": {
      "initialVisibility": "visible"
    }
  },
  "disclosure-trigger#description-mobile": {
    "children": ["rich-text#description-mobile-accordion-text", "disclosure-state-indicator#a"],
    "props": {
      "blockClass": "accordion-pdp-desktop-trigger"
    }
  },
  "disclosure-content#description-mobile": {
    "children": ["flex-layout.row#description-mobile"]
  },
  "rich-text#description-mobile-accordion-text": {
    "props": {
      "text": "Descrição",
      "blockClass": "accordion-pdp-desktop-trigger-text"
    }
  },

  "disclosure-layout#ficha-tecnica-mobile": {
    "children": ["disclosure-trigger#ficha-tecnica-mobile", "disclosure-content#ficha-tecnica-mobile"],
    "props": {
      "initialVisibility": "visible"
    }
  },
  "disclosure-trigger#ficha-tecnica-mobile": {
    "children": ["rich-text#ficha-tecnica-mobile-accordion-text", "disclosure-state-indicator#a"],
    "props": {
      "blockClass": "accordion-pdp-desktop-trigger"
    }
  },
  "disclosure-content#ficha-tecnica-mobile": {
    "children": [
      "rich-text#data-sheet-mobile",
      "flex-layout.row#data-sheet",
      "rich-text#pdp-datasheet-text-mobile"
    ]
  },
  "rich-text#ficha-tecnica-mobile-accordion-text": {
    "props": {
      "text": "Ficha técnica",
      "blockClass": "accordion-pdp-desktop-trigger-text"
    }
  },

  "disclosure-layout#conteudo-especial-mobile": {
    "children": ["disclosure-trigger#conteudo-especial-mobile", "disclosure-content#conteudo-especial-mobile"],
    "props": {
      "initialVisibility": "visible"
    }
  },
  "disclosure-trigger#conteudo-especial-mobile": {
    "children": ["rich-text#conteudo-especial-mobile-accordion-text", "disclosure-state-indicator#a"],
    "props": {
      "blockClass": "accordion-pdp-desktop-trigger--conteudo-especial"
    }
  },
  "disclosure-content#conteudo-especial-mobile": {
    "children": ["flex-layout.row#conteudo-especial-mobile"],
    "props": {
      "blockClass": "conteudo-especial"
    }
  },
  "rich-text#conteudo-especial-mobile-accordion-text": {
    "props": {
      "text": "Detalhes do produto",
      "blockClass": "accordion-pdp-desktop-trigger-text"
    }
  },

  "disclosure-state-indicator#a": {
    "props": {
      "Show": "rich-text#trigger-open",
      "Hide": "rich-text#trigger-close"
    }
  }
}
