{
  "store.search": {
    "blocks": ["search-result-layout"],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip",
        "maxItemsPerPage": 20
      }
    }
  },

  "store.search#brand": {
    "blocks": ["search-result-layout"],
    "props": {
      "context": {
        "orderByField": "OrderByReleaseDateDESC",
        "hideUnavailableItems": true,
        "maxItemsPerPage": 20,
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip"
      }
    }
  },

  "store.search#department": {
    "blocks": ["search-result-layout#department"],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip",
        "maxItemsPerPage": 20
      }
    }
  },

  "search-result-layout#department": {
    "blocks": [
      "search-result-layout.desktop#department",
      "search-result-layout.mobile#department",
      "search-not-found-layout"
    ]
  },

  "search-result-layout.desktop#department": {
    "children": [
      "flex-layout.row#search-banner",
      "flex-layout.row#banner-links",
      "flex-layout.row#result",
      "flex-layout.row#fetchmore",
      "flex-layout.row#shelf-offers-row",
      "rich-text#department-heading",
      "rich-text#department"
    ],
    "title": "desktop",
    "props": {
      "pagination": "show-more",
      "preventRouteChange": false,
      "mobileLayout": {
        "mode1": "normal",
        "mode2": "small"
      }
    }
  },
  "rich-text#department-heading": {
    "props": {
      "text": "",
      "blockClass": "departament-heading"
    },
    "title": "heading departamento"
  },
  "rich-text#department": {
    "props": {
      "text": "",
      "blockClass": "departament-text"
    },
    "title": "seo text"
  },
  "search-result-layout.mobile#department": {
    "children": [
      "flex-layout.row#search-banner",
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "flex-layout.row#shelf-offers-row",
      "rich-text#department-heading",
      "rich-text#department"
    ],
    "title": "Mobile",
    "props": {
      "pagination": "show-more",
      "mobileLayout": {
        "mode1": "normal",
        "mode2": "small"
      }
    }
  },

  "store.search#category": {
    "blocks": ["flex-layout.row#search-banner", "search-result-layout"],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip",
        "maxItemsPerPage": 20
      }
    }
  },

  "store.search#subcategory": {
    "blocks": ["flex-layout.row#search-banner", "search-result-layout"],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip",
        "maxItemsPerPage": 20
      }
    }
  },

  "search-result-layout": {
    "blocks": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },

  "search-result-layout.desktop": {
    "children": [
      // "breadcrumb.search",
      "flex-layout.row#search-banner",
      "flex-layout.row#banner-links",
      "flex-layout.row#result",
      "flex-layout.row#fetchmore",
      "flex-layout.row#shelf-offers-row",
      "rich-text#department-heading",
      "rich-text#department"
    ],
    "title": "desktop",
    "props": {
      "pagination": "show-more",
      "preventRouteChange": false,
      "mobileLayout": {
        "mode1": "normal",
        "mode2": "small"
      }
    }
  },

  "flex-layout.row#search-banner": {
    "children": ["info-card#search-banner"],
    "props": {
      "blockClass": "search-row",
      "fullWidth": true,
      "marginBottom": 3
    },
    "title": "Search Banner"
  },

  "info-card#search-banner": {
    "props": {
      "isFullModeStyle": false,
      "textPosition": "center",
      "callToActionMode": "none",
      "headline": "",
      "blockClass": "searchBanner",
      "textAlignment": "center"
    },
    "title": "Banner"
  },

  "flex-layout.row#banner-links": {
    "children": ["rich-text#search-links"],
    "props": {
      "blockClass": "banner-links-container",
      "fullWidth": true
    },
    "title":  "Banner links container"
  },

  "rich-text#search-links": {
    "props": {
      "blockClass": "banner-links-links"
    },
    "title":  "Banner links"
  },

  "flex-layout.row#searchbread": {
    "children": ["breadcrumb.search"],
    "props": {
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "blockClass": "breadcrumb"
    }
  },
